{
  "name": "@opencanoetiming/timing-design-system",
  "version": "0.3.3",
  "description": "Canoe Timing Design System - UI komponenty pro vodáckou časomíru",
  "author": "Open Canoe Timing Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenCanoeTiming/timing-design-system.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "type": "module",
  "main": "dist/timing.css",
  "exports": {
    ".": "./src/react/index.ts",
    "./react": "./src/react/index.ts",
    "./css": "./dist/timing.css",
    "./css/min": "./dist/timing.min.css",
    "./dist/*": "./dist/*"
  },
  "types": "./src/react/index.ts",
  "files": [
    "dist/",
    "src/tokens/",
    "src/css/",
    "src/fonts/",
    "src/react/"
  ],
  "scripts": {
    "dev": "npx serve . -p 3000 --cors",
    "build": "npm run build:css",
    "build:css": "node scripts/build-css.js",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'CSS linting not configured yet'"
  },
  "devDependencies": {
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "keywords": [
    "design-system",
    "css",
    "canoe",
    "slalom",
    "timing"
  ]
}
